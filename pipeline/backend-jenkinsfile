@Library('pipeline-library@master') _
 
pipeline {
    agent any
    triggers {
        pollSCM '* * * * *'
    }
    stages {
        stage('Git Checkout') {
            steps {
            gitCheckout(
                branch: "master",
                url: "https://github.com/vigneshsweekaran/capstone-easyclaim-backend.git",
                credential: "git_credential")
            }
        }
    }
}
